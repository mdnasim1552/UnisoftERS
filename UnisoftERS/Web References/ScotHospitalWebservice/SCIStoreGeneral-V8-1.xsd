              </xsd:annotation>
            </xsd:element>
            <xsd:element name="Provider" type="sci:ProviderItem" />
            <xsd:any minOccurs="0" maxOccurs="unbounded" namespace="##other" processContents="lax" />
          </xsd:sequence>
        </xsd:complexType>
