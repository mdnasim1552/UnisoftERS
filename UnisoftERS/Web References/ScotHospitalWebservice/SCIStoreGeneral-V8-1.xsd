  <xsd:complexType name="InvestigationReportMessageType">
    <xsd:annotation>
      <xsd:documentation>Returned during getResult. A structure for data relating to the message contained in the Investigation  Report, as opposed to the data about the document. Ref:  ENV 12838:1996 6.3.30.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:annotation>
        <xsd:documentation>The organisation (eg laboratory) providing the service  Ref:ENV-13838:1996 P66</xsd:documentation>
      </xsd:annotation>
      <xsd:element name="ReportIdentifier" type="gen:ID_VALUE_TYPE" />
      <xsd:element name="ReportDate" type="xsd:date">
        <xsd:annotation>
          <xsd:documentation>Date of report</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="ReportTime" type="xsd:time">
        <xsd:annotation>
          <xsd:documentation>Time of report</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Discipline" type="sci:ReportDiagnosticService" />
      <xsd:element minOccurs="0" name="SectionName" type="sci:ReportDiagnosticService" />
      <xsd:element name="ServiceProvider">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element minOccurs="0" name="ProvidingSite" type="gen:ORGANISATION_TYPE">
              <xsd:annotation>
                <xsd:documentation>This is the physical site that is providing the service (Hospital/clinic)</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="ProvidingLocation" type="gen:ORGANISATION_TYPE">
              <xsd:annotation>
                <xsd:documentation>This is the location within the site that is providing the service (Ward/room)</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="RequestingParty" type="gen:HCP_DETAIL_TYPE">
        <xsd:annotation>
          <xsd:documentation>The Health Care Professional who orders a service  Ref : Pathology Messaging Enabler Project V1.00 (implied)</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="ConfidentialityStatus" type="gen:CONFIDENTIALITY_STATUS">
        <xsd:annotation>
          <xsd:documentation>Indicator of whether the data is particularly sensitive or not. Ref: Order Communication Technical Reference Manual (Northgate) </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="ServiceRequest" type="gen:SERVICE_REQUEST_TYPE">
        <xsd:annotation>
          <xsd:documentation>This is a container for the details required for requesting the service  Ref:  ENV - 12838:1996</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" maxOccurs="unbounded" name="ServiceResult" type="sci:ResultSuite">
        <xsd:annotation>
          <xsd:documentation>This is a container for the details required for requesting the service  Ref:  ENV - 12838:1996</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="PatientInformation" type="sci:PatientInformation">
        <xsd:annotation>
          <xsd:documentation>Container for patient identifying details of the patient who is the subject of the document.  Ref: ENV 13606 - 4:2000 7.4.6  </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="PatientClass" type="xsd:string" />
      <xsd:element name="ReportAbnormal" type="xsd:boolean" />
      <xsd:any minOccurs="0" maxOccurs="unbounded" namespace="##other" processContents="lax" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="STRUCTURED_REFERRAL_REASON_TYPE">
    <xsd:annotation>
      <xsd:documentation>A structure for describing the reason for referral.</xsd:documentation>
      <xsd:appinfo>SIGN Report on a Recommended Referral Document November 1998</xsd:appinfo>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="CareTypeRequested">
        <xsd:annotation>
          <xsd:documentation>The type of care that is being requested. SIGN examples indicate an expectation of types of service e.g. "Inpatient Care", "Day Case Care".  It is NOT expected that these will be implemented as a list of values in the near future.</xsd:documentation>
          <xsd:appinfo>SIGN Report on Recommendation Referral Document November 1998</xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:minLength value="1" />
            <xsd:maxLength value="70" />
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="ExpectedOutcome">
        <xsd:annotation>
          <xsd:documentation>The type of outcome that is expected. SIGN examples indicate an expectation of types of healthcare activity e.g. 'consultation', 'assessment'.  It is NOT expected that these will be implemented as a list of values in the near future.</xsd:documentation>
          <xsd:appinfo>SIGN Report on Recommendation Referral Document November 1998</xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:minLength value="1" />
            <xsd:maxLength value="70" />
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:any minOccurs="0" maxOccurs="unbounded" namespace="##other" processContents="lax" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="REFERRAL_REASON_TYPE">
    <xsd:annotation>
      <xsd:documentation>A structure for data pertaining to a referral reason.</xsd:documentation>
      <xsd:appinfo>SIGN Report on a Recommended Referral Document November 1998</xsd:appinfo>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:choice>
        <xsd:element name="StructuredReferralReason" type="sci:STRUCTURED_REFERRAL_REASON_TYPE" />
        <xsd:element name="UnstructuredReferralReason">
          <xsd:annotation>
            <xsd:documentation>The reason for referral expressed in an unstructured format.</xsd:documentation>
            <xsd:appinfo>SIGN Report on Recommendation Referral Document November 1998</xsd:appinfo>
          </xsd:annotation>
          <xsd:simpleType>
            <xsd:restriction base="xsd:string">
              <xsd:minLength value="1" />
              <xsd:maxLength value="198" />
            </xsd:restriction>
          </xsd:simpleType>
        </xsd:element>
      </xsd:choice>
      <xsd:element minOccurs="0" name="Comment" type="xsd:normalizedString">
        <xsd:annotation>
          <xsd:documentation>A piece of free text. Used generally to contain information which is supplementary to the structured area. Ref: ???? </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:any minOccurs="0" maxOccurs="unbounded" namespace="##other" processContents="lax" />
    </xsd:sequence>
  </xsd:complexType>
