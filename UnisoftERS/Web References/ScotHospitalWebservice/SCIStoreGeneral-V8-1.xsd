<?xml version="1.0" encoding="utf-8"?>
<xsd:schema xmlns:gen="http://www.show.scot.nhs.uk/isd/General" xmlns:sci="http://www.show.scot.nhs.uk/isd/SCIStore" attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="http://www.show.scot.nhs.uk/isd/SCIStore" version="8.1" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <xsd:import schemaLocation="general-v2-10a.xsd" namespace="http://www.show.scot.nhs.uk/isd/General" />
  <xsd:annotation>
    <xsd:documentation>Definition of components for SCIStore message types. Uses: general-v2-10a.xsd
		</xsd:documentation>
  </xsd:annotation>
  <xsd:simpleType name="RecordModifcationType">
    <xsd:annotation>
      <xsd:documentation>Values are:
I - Insert
A - Amend
D - Delete
C - Confirm
</xsd:documentation>
    </xsd:annotation>
    <xsd:union>
      <xsd:simpleType>
        <xsd:restriction base="xsd:string">
          <xsd:enumeration value="I" />
          <xsd:enumeration value="A" />
          <xsd:enumeration value="D" />
          <xsd:enumeration value="C" />
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:union>
  </xsd:simpleType>
  <xsd:simpleType name="MergeAction">
    <xsd:annotation>
      <xsd:documentation>Values are:
Merge
UnMerge
</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:minLength value="1" />
      <xsd:maxLength value="35" />
      <xsd:enumeration value="UnMerge" />
      <xsd:enumeration value="Merge" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="DocumentData">
    <xsd:annotation>
      <xsd:documentation>Document Data type for base64</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:base64Binary" />
  </xsd:simpleType>
  <xsd:simpleType name="GMCcode">
    <xsd:annotation>
      <xsd:documentation>definition of GMC code</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:minLength value="0" />
      <xsd:maxLength value="20" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="BreakGlassTypeCode">
    <xsd:annotation>
      <xsd:documentation>definition of BreakGlassTypeCode</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:minLength value="1" />
      <xsd:maxLength value="10" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="CHInumber">
    <xsd:annotation>
      <xsd:documentation>definition of CHI number</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:minLength value="1" />
      <xsd:maxLength value="10" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="MessageQueueName">
    <xsd:annotation>
      <xsd:documentation>standard definition of message Queue name</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string" />
  </xsd:simpleType>
  <xsd:simpleType name="NotificationID">
    <xsd:annotation>
      <xsd:documentation>Notification identifier</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string" />
  </xsd:simpleType>
  <xsd:simpleType name="ProviderSearchType">
    <xsd:annotation>
      <xsd:documentation>search provider valid list</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="MergedResults" />
      <xsd:enumeration value="FirstHit" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="SearchComparator">
    <xsd:annotation>
      <xsd:documentation>search comparitor valid list</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="equals" />
      <xsd:enumeration value="begins" />
      <xsd:enumeration value="contains" />
      <xsd:enumeration value="soundex" />
      <xsd:enumeration value="index equals" />
      <xsd:enumeration value="index begins" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="PatientIdentifier">
    <xsd:annotation>
      <xsd:documentation>generic definition for Patient identifier</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:maxLength value="15" />
      <xsd:minLength value="1" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="ReferralUrgencySource">
    <xsd:annotation>
      <xsd:documentation>generic definition for Referral dataset source id</xsd:documentation>
    </xsd:annotation>
    <xsd:union>
      <xsd:simpleType>
        <xsd:restriction base="xsd:positiveInteger">
          <xsd:enumeration value="1" />
          <xsd:enumeration value="2" />
          <xsd:enumeration value="3" />
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType>
        <xsd:restriction base="xsd:positiveInteger" />
      </xsd:simpleType>
    </xsd:union>
  </xsd:simpleType>
  <xsd:simpleType name="PatientCareTypeDescription">
    <xsd:annotation>
      <xsd:documentation>A stipulation of patient type description.</xsd:documentation>
    </xsd:annotation>
    <xsd:union>
      <xsd:simpleType>
        <xsd:restriction base="xsd:string">
          <xsd:enumeration value="Inpatient" />
          <xsd:enumeration value="Outpatient" />
          <xsd:enumeration value="Daycase" />
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType>
        <xsd:restriction base="xsd:string">
          <xsd:minLength value="1" />
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:union>
  </xsd:simpleType>
  <xsd:simpleType name="PatientCareTypeCode">
    <xsd:annotation>
      <xsd:documentation>A stipulation of patient type code.</xsd:documentation>
    </xsd:annotation>
    <xsd:union>
      <xsd:simpleType>
        <xsd:restriction base="xsd:string">
          <xsd:enumeration value="I" />
          <xsd:enumeration value="O" />
          <xsd:enumeration value="D" />
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType>
        <xsd:restriction base="xsd:string">
          <xsd:minLength value="1" />
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:union>
  </xsd:simpleType>
  <xsd:simpleType name="UrgencyValue">
    <xsd:annotation>
      <xsd:documentation>A stipulation by the referring practitioner regarding how urgently a patient should be seen.</xsd:documentation>
      <xsd:appinfo>SIGN Report on a Recommended Referral Document November 1998</xsd:appinfo>
    </xsd:annotation>
    <xsd:union>
      <xsd:simpleType>
        <xsd:restriction base="xsd:string">
          <xsd:enumeration value="Urgent" />
          <xsd:enumeration value="Soon" />
          <xsd:enumeration value="Routine" />
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType>
        <xsd:restriction base="xsd:string">
          <xsd:minLength value="1" />
          <xsd:maxLength value="35" />
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:union>
  </xsd:simpleType>
  <xsd:simpleType name="UrgencyReason">
    <xsd:annotation>
      <xsd:documentation>A specific reason given by the referring practitioner if the UrgencyValue content is anything other than "Routine".  *Note : But mandatory if Urgency Value is "Urgent" or "Soon" .</xsd:documentation>
      <xsd:appinfo>SIGN Report on a Recommended Referral Document November 1998</xsd:appinfo>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:minLength value="1" />
      <xsd:maxLength value="198" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="RequestStatus">
    <xsd:annotation>
      <xsd:documentation>The status of a treatment request.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Outstanding" />
      <xsd:enumeration value="Completed" />
      <xsd:enumeration value="Cancelled" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="ReferralSourceCode">
    <xsd:annotation>
      <xsd:documentation>generic definition for Referral dataset source code</xsd:documentation>
    </xsd:annotation>
    <xsd:union>
      <xsd:simpleType>
        <xsd:restriction base="xsd:positiveInteger">
          <xsd:enumeration value="1" />
          <xsd:enumeration value="2" />
          <xsd:enumeration value="3" />
          <xsd:enumeration value="4" />
          <xsd:enumeration value="5" />
          <xsd:enumeration value="7" />
          <xsd:enumeration value="8" />
          <xsd:enumeration value="11" />
          <xsd:enumeration value="12" />
          <xsd:enumeration value="13" />
          <xsd:enumeration value="99" />
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType>
        <xsd:restriction base="xsd:positiveInteger" />
      </xsd:simpleType>
    </xsd:union>
  </xsd:simpleType>
  <xsd:simpleType name="StoreGenericIdentifier">
    <xsd:annotation>
      <xsd:documentation>Generic internal Identifier</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:nonNegativeInteger" />
  </xsd:simpleType>
  <xsd:simpleType name="SubscriptionID">
    <xsd:annotation>
      <xsd:documentation>Subscription identifier</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:minLength value="1" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="NotificationType">
    <xsd:annotation>
      <xsd:documentation>Notification type</xsd:documentation>
    </xsd:annotation>
    <xsd:union>
      <xsd:simpleType>
        <xsd:restriction base="xsd:string">
          <xsd:enumeration value="PATIENT" />
          <xsd:enumeration value="RESULT" />
          <xsd:enumeration value="TREATMENTLOG" />
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType>
        <xsd:restriction base="xsd:string">
          <xsd:minLength value="1" />
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:union>
  </xsd:simpleType>
  <xsd:simpleType name="SubscriptionType">
    <xsd:annotation>
      <xsd:documentation>Subscription type</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="PATIENT" />
      <xsd:enumeration value="RESULT" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="Token">
    <xsd:annotation>
      <xsd:documentation>Definition of login token</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:minLength value="1" />
      <xsd:maxLength value="50" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="ADTrecordIdentifier">
    <xsd:annotation>
      <xsd:documentation>Unique Identifier used for ADT type transactions</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:minLength value="1" />
      <xsd:maxLength value="35" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="OutpatientRecordIdentifier">
    <xsd:annotation>
      <xsd:documentation>ADT Unique Identifier</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:minLength value="1" />
      <xsd:maxLength value="35" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="OpenClosedStatus">
    <xsd:annotation>
      <xsd:documentation>Status type for generic open closed status</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Open" />
      <xsd:enumeration value="Closed" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="ReportDiagnosticService">
    <xsd:annotation>
      <xsd:documentation>Report type or diagnostic service</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:minLength value="1" />
      <xsd:maxLength value="50" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="FindResultGroupByType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="ResultSet" />
      <xsd:enumeration value="Sample" />
      <xsd:enumeration value="Report" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="ProviderIdentifier">
    <xsd:annotation>
      <xsd:documentation>Definition of ProviderIdentifier</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:minLength value="1" />
      <xsd:maxLength value="20" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="PatientMatchType">
    <xsd:annotation>
      <xsd:documentation>Enum of reposes to a LocationPatient</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="ZERO" />
      <xsd:enumeration value="ONE" />
      <xsd:enumeration value="MANY" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="ClinicalRecordType">
    <xsd:annotation>
      <xsd:documentation>type of clinical information  i.e. results, document, adt etc</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:maxLength value="50" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:element name="DisplayHTML" type="xsd:string">
    <xsd:annotation>
      <xsd:documentation>Embedded Display HTML element</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:complexType name="UniqueCarePathwayNumberDataType">
    <xsd:annotation>
      <xsd:documentation>A general type to represent a a ucpn and the datasets that contain ucpns</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="UniqueCarePathwayNumber" type="gen:UNIQUE_CARE_PATHWAY_NUMBER_TYPE" />
      <xsd:element minOccurs="0" name="FirstEncounteredDate" type="xsd:date" />
      <xsd:element minOccurs="0" name="FirstEncounteredTime" type="xsd:time" />
      <xsd:element minOccurs="0" name="LastEncounteredDate" type="xsd:date" />
      <xsd:element minOccurs="0" name="LastEncounteredTime" type="xsd:time" />
      <xsd:element minOccurs="0" maxOccurs="unbounded" name="UCPNusage" type="sci:UniqueCarePathwayNumberUsageType" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="PATIENT_CARE_TYPE">
    <xsd:annotation>
      <xsd:documentation>Defines the structure for holding a description and or code for patient care type: Inpatient, Outpatient, Daycase</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" name="Description" type="sci:PatientCareTypeDescription">
        <xsd:annotation>
          <xsd:documentation>Patient care type description</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Code" type="sci:PatientCareTypeCode">
        <xsd:annotation>
          <xsd:documentation>Patient care type code</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="UniqueCarePathwayNumberUsageType">
    <xsd:annotation>
      <xsd:documentation>A general type to represent a a ucpn and the datasets that contain ucpns</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="DataType">
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:minLength value="1" />
            <xsd:maxLength value="35" />
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element minOccurs="0" name="FirstEncounteredDate" type="xsd:date" />
      <xsd:element minOccurs="0" name="FirstEncounteredTime" type="xsd:time" />
      <xsd:element minOccurs="0" name="LastEncounteredDate" type="xsd:date" />
      <xsd:element minOccurs="0" name="LastEncounteredTime" type="xsd:time" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="GENERAL_CODE_TYPE">
    <xsd:annotation>
      <xsd:documentation>A general type to represent a code/description pair.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" name="CodeDescription">
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:minLength value="1" />
            <xsd:maxLength value="100" />
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element minOccurs="0" name="CodeValue">
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:minLength value="1" />
            <xsd:maxLength value="10" />
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="MESSAGE_HEADER_TYPE">
    <xsd:annotation>
      <xsd:documentation>A generic header structure for data relating to an electronic message</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element maxOccurs="unbounded" name="MessageIdentifier" type="gen:ID_TYPE">
        <xsd:annotation>
          <xsd:documentation>Identification of message</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="OriginatingSystem" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>This element represents an identifier of the originating system. This should be unique for each system that originates a message</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="MessageCreationDate" type="xsd:date">
        <xsd:annotation>
          <xsd:documentation>Date of creation of message. Only date implemented here.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="MessageCreationTime" type="xsd:time">
        <xsd:annotation>
          <xsd:documentation>Date of creation of message.  Only time implemented here.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:any minOccurs="0" maxOccurs="unbounded" namespace="##other" processContents="lax" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="CLINICAL_INDEX_TYPE">
    <xsd:annotation>
      <xsd:documentation>Category type with subtype optional to allow for sci store where subtype is not required  </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="DocumentType">
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:minLength value="1" />
            <xsd:maxLength value="70" />
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element fixed="|" name="Separator" type="xsd:string" />
      <xsd:element minOccurs="0" name="DocumentSubType">
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:minLength value="1" />
            <xsd:maxLength value="70" />
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="DateRangeSelection">
    <xsd:annotation>
      <xsd:documentation>Standard Date Range Selection</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" name="DateFrom" type="xsd:date" />
      <xsd:element minOccurs="0" name="DateTo" type="xsd:date" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="FindDateRange">
    <xsd:annotation>
      <xsd:documentation>Date Range for FindResults</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="DateFrom" type="xsd:date" />
      <xsd:element name="DateTo" type="xsd:date" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="FindDateTimeRange">
    <xsd:annotation>
      <xsd:documentation>Datetime Range for FindResults</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="DateFrom" type="xsd:dateTime" />
      <xsd:element name="DateTo" type="xsd:dateTime" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="MedicalFacility">
    <xsd:annotation>
      <xsd:documentation>MedicalFacility</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="Code">
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="35" />
            <xsd:minLength value="1" />
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element minOccurs="0" name="Description">
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:minLength value="1" />
            <xsd:maxLength value="35" />
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:any minOccurs="0" maxOccurs="unbounded" namespace="##other" processContents="lax" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="UserInfo">
    <xsd:annotation>
      <xsd:documentation>Standard User Info Block</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="UserName">
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:minLength value="1" />
            <xsd:maxLength value="20" />
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="FriendlyName">
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:minLength value="1" />
            <xsd:maxLength value="100" />
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="SystemCode">
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:minLength value="1" />
            <xsd:maxLength value="20" />
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element minOccurs="0" name="SystemLocation">
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:minLength value="1" />
            <xsd:maxLength value="40" />
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="ProviderList">
    <xsd:annotation>
      <xsd:documentation>List of provider items</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" maxOccurs="unbounded" name="Provider" type="sci:ProviderItem" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="ProviderItem">
    <xsd:annotation>
      <xsd:documentation>Provider item </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="ProviderIdentifier">
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:minLength value="1" />
            <xsd:maxLength value="20" />
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:any minOccurs="0" maxOccurs="unbounded" namespace="##other" processContents="lax" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="FindUserCriteria">
    <xsd:annotation>
      <xsd:documentation>Standard User Info Selection Block</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" name="UserName">
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:minLength value="1" />
            <xsd:maxLength value="50" />
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element minOccurs="0" name="SystemCode">
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:minLength value="1" />
            <xsd:maxLength value="20" />
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element minOccurs="0" name="SystemLocation">
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:minLength value="1" />
            <xsd:maxLength value="40" />
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="OutPatientInterface">
    <xsd:annotation>
      <xsd:documentation>Detailed outpatient information</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="OutPatientMessageContent" type="sci:OutpatientInfo">
        <xsd:annotation>
          <xsd:documentation>Element is not present for transactions of Type "Deletion"</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="PatientInformation" type="sci:PatientInformation" />
      <xsd:element minOccurs="0" name="TransactionType" type="sci:RecordModifcationType" />
      <xsd:any minOccurs="0" maxOccurs="unbounded" namespace="##other" processContents="lax" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="OutpatientInfo">
    <xsd:annotation>
      <xsd:documentation>Outpatient type definition</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="AppointmentID" type="sci:OutpatientRecordIdentifier" />
      <xsd:element name="AppointmentSlot" type="gen:SLOT_TYPE" />
      <xsd:element name="Status">
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="2" />
            <xsd:minLength value="1" />
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="AttendanceType" type="xsd:string" />
      <xsd:element name="CancellationReason" type="xsd:string" />
      <xsd:element name="OutpatientCarePoint" type="sci:OutpatientCarepoint" />
      <xsd:element minOccurs="0" name="AdmissionID" type="sci:ADTrecordIdentifier" />
      <xsd:element minOccurs="0" name="PatientCategory" type="xsd:integer">
        <xsd:annotation>
          <xsd:documentation>Patient Category (COPPISH SMR Data Manual version 1.1 Episode Management Section item 9 p 3-20)</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="PatientType" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>Field is derived in the PAS interface; values I (Inpatient) or D (Daycase)</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="TransactionDetails">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="TransactionDateTime" nillable="true" type="xsd:dateTime" />
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:any minOccurs="0" maxOccurs="unbounded" namespace="##other" processContents="lax" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="OutpatientCarepoint">
    <xsd:annotation>
      <xsd:documentation>outpatient patient care point definition</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" name="ConsultantID" type="gen:ID_TYPE" />
      <xsd:element minOccurs="0" name="Specialty" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>COPPISH code</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Facility" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>COPPISH code</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:any minOccurs="0" maxOccurs="unbounded" namespace="##other" processContents="lax" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="ADTadmissionType">
    <xsd:annotation>
      <xsd:documentation>ADT admission type definition</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="AdmissionDateTime" nillable="true" type="xsd:dateTime" />
      <xsd:element minOccurs="0" name="Status">
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:minLength value="1" />
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element minOccurs="0" name="AdmissionFromCode">
        <xsd:annotation>
          <xsd:documentation>Admission/Transfer From code on admission to hospital (COPPISH SMR Data Manual version 1.1 Episode Management Section item 16 pp 3-37-3-39)</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="2" />
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element minOccurs="0" name="AdmittedCode" type="xsd:integer">
        <xsd:annotation>
          <xsd:documentation>Admission Type (COPPISH SMR Data Manual version 1.1 Episode Management Section item 14 pp 3-31-3-33)</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="AdmissionReason">
        <xsd:annotation>
          <xsd:documentation>Admission Reason</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="2" />
            <xsd:minLength value="1" />
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element minOccurs="0" name="PatientCategory" type="xsd:integer">
        <xsd:annotation>
          <xsd:documentation>Patient Category (COPPISH SMR Data Manual version 1.1 Episode Management Section item 9 p 3-20)</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="PatientType" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>Field is derived in the PAS interface; values I (Inpatient) or D (Daycase)</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="AdmissionCarePoint" type="sci:ADTpatientCarePoint" />
      <xsd:any minOccurs="0" maxOccurs="unbounded" namespace="##other" processContents="lax" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="ADTtransferType">
    <xsd:annotation>
      <xsd:documentation>ADT transfer type definition</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="TransferDateTime" type="xsd:dateTime" />
      <xsd:element minOccurs="0" name="TransferCode" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>Transfer Type (free text)</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="PatientCategory" type="xsd:integer">
        <xsd:annotation>
          <xsd:documentation>Patient Category (COPPISH SMR Data Manual version 1.1 Episode Management Section item 9 p 3-20)</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="PatientType" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>Field is derived in the PAS interface; values I (Inpatient) or D (Daycase)</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="PreCarePoint" type="sci:ADTpatientCarePoint" />
      <xsd:element name="PostCarePoint" type="sci:ADTpatientCarePoint" />
      <xsd:any minOccurs="0" maxOccurs="unbounded" namespace="##other" processContents="lax" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="ADTdischargeType">
    <xsd:annotation>
      <xsd:documentation>ADT discharge type definition</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="DischargeDateTime" nillable="true" type="xsd:dateTime" />
      <xsd:element name="DischargeCode" type="xsd:integer">
        <xsd:annotation>
          <xsd:documentation>Discharge Type (COPPISH SMR Data Manual version 1.1 Episode Management Section item 21 pp 3-45-3-46)</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="DischargeDescription">
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:minLength value="1" />
            <xsd:maxLength value="100" />
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element minOccurs="0" name="DateReadyForDischarge" nillable="true" type="xsd:dateTime" />
      <xsd:element minOccurs="0" name="DischargeDestination" type="xsd:integer">
        <xsd:annotation>
          <xsd:documentation>Discharge/Transfer To(COPPISH SMR Data Manual version 1.1 Episode Management Section)</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="PatientCategory" type="xsd:integer">
        <xsd:annotation>
          <xsd:documentation>Patient Category (COPPISH SMR Data Manual version 1.1 Episode Management Section item 9 p 3-20)</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="PatientType" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>Field is derived in the PAS interface; values I (Inpatient) or D (Daycase)</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="DischargeCarePoint" type="sci:ADTpatientCarePoint" />
      <xsd:any minOccurs="0" maxOccurs="unbounded" namespace="##other" processContents="lax" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="WAITING_TIMES_DATA_TYPE">
    <xsd:annotation>
      <xsd:documentation>Complex type to hold referral data for calculation of waiting times under the New Ways for Waiting Times initiative.Ref: New Ways of Measuring and Monitoring Waiting Times for NHS Services in Scotland 'The New Ways Project' Specification of Data Required and its extraction v1.1 28 Feb 2007</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" name="PatientAvailableAtShortNotice" type="xsd:boolean">
        <xsd:annotation>
          <xsd:documentation>Is the patient available to attend or be admitted at short notice. Ref: New Ways of Measuring and Monitoring Waiting Times for NHS Services in Scotland 'The New Ways Project' Specification of Data Required and its extraction v1.1 28 Feb 2007</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="PatientSuitableForPooledList" type="xsd:boolean">
        <xsd:annotation>
          <xsd:documentation>Is the patient suitable for being part of a pooled list.Ref: New Ways of Measuring and Monitoring Waiting Times for NHS Services in Scotland 'The New Ways Project' Specification of Data Required and its extraction v1.1 28 Feb 2007</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="PatientWillingToChangeClinician" type="xsd:boolean">
        <xsd:annotation>
          <xsd:documentation>Is the patient willing to change clinician.Ref: New Ways of Measuring and Monitoring Waiting Times for NHS Services in Scotland 'The New Ways Project' Specification of Data Required and its extraction v1.1 28 Feb 2007</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="PatientWillingToChangeHealthBoard" type="xsd:boolean">
        <xsd:annotation>
          <xsd:documentation>Is the patient willing to change health board.Ref: New Ways of Measuring and Monitoring Waiting Times for NHS Services in Scotland 'The New Ways Project' Specification of Data Required and its extraction v1.1 28 Feb 2007</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:any minOccurs="0" maxOccurs="unbounded" namespace="##other" processContents="lax" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="TreatmentLog">
    <xsd:annotation>
      <xsd:documentation>Detailed TreatmentLog information</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="RecordIdentifier" type="sci:ADTrecordIdentifier" />
      <xsd:element minOccurs="0" name="PatientInformation" type="sci:PatientInformation" />
      <xsd:element minOccurs="0" name="TreatmentRequestPrf" type="sci:TreatmentRequestInfo">
        <xsd:annotation>
          <xsd:documentation>Container for data relating to the  functions associated with the request for treatment.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="RequestStatusPrf" type="sci:TreatmentRequestStatusType">
        <xsd:annotation>
          <xsd:documentation>Container for data relating to the overall status of the request to treat. This will include request state (open and closed) and request status (outstanding, completed and cancelled)</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="AttendanceRequestedPrf" type="sci:AttendanceRequestedType">
        <xsd:annotation>
          <xsd:documentation>Container for data relating to the attendance details requested for a treatment.  Including treatment care point, target date to come in and target breach date</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="RequestOutcomePrf" type="sci:TreatmentRequestOutcomeType">
        <xsd:annotation>
          <xsd:documentation>Container for data relating to the overall outcome relating to the request to treat. This will include closure information and any actual attendance information</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="DeliveryStatusPrf" type="sci:TreatmentRequestDeliveryScheduleType">
        <xsd:annotation>
          <xsd:documentation>This container is for data relating to the delivery of the treatment with regard to RTT.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:any minOccurs="0" maxOccurs="unbounded" namespace="##other" processContents="lax" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="TreatmentRequestInfo">
    <xsd:annotation>
      <xsd:documentation>Container for data relating to the  functions associated with the request for treatment.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="PatientCareType" type="sci:PATIENT_CARE_TYPE" />
      <xsd:element minOccurs="0" name="PatientCategory" type="sci:GENERAL_CODE_TYPE">
        <xsd:annotation>
          <xsd:documentation>Determines the category the patient falls into, codes and description should be taken from the ISD List for Patient Category: Amenity, Paying, NHS etc..</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="RequestDate" type="xsd:date">
        <xsd:annotation>
          <xsd:documentation>Date of request</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="RequestTime" type="xsd:time">
        <xsd:annotation>
          <xsd:documentation>Time of request</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="TreatmentIdentifiers" type="sci:TreatmentRequestIdentifiers" />
      <xsd:element minOccurs="0" name="TreatmentRequested" type="gen:CLINICAL_CIRCUMSTANCE_TYPE" />
      <xsd:element minOccurs="0" name="AssessedUrgencyValue" type="sci:UrgencyValue" />
      <xsd:element minOccurs="0" name="AssessedUrgencyReason" type="sci:UrgencyReason" />
      <xsd:element minOccurs="0" name="Comment" type="gen:COMMENT_TYPE">
        <xsd:annotation>
          <xsd:documentation>Other relevant unstructured commentary on the request. </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="ReferralDetails">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element minOccurs="0" name="ReferralIdentifier" type="gen:ID_TYPE" />
            <xsd:element minOccurs="0" name="ReferralReason" type="sci:REFERRAL_REASON_TYPE" />
            <xsd:element minOccurs="0" name="ReferralUrgencyValue" type="sci:UrgencyValue">
              <xsd:annotation>
                <xsd:documentation>A stipulation by a practitioner regarding how urgently a patient should be seen.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="ReferralUrgencyReason" type="sci:UrgencyReason">
              <xsd:annotation>
                <xsd:documentation>A specific reason given by a practitioner if the UrgencyValue content is anything other than "Routine".  *Note : But mandatory if Urgency Value is "Urgent" or "Soon" .</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:any minOccurs="0" maxOccurs="unbounded" namespace="##other" processContents="lax" />
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element minOccurs="0" name="WaitingTimesData" type="sci:WAITING_TIMES_DATA_TYPE" />
      <xsd:any minOccurs="0" maxOccurs="unbounded" namespace="##other" processContents="lax" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="AttendanceRequestedType">
    <xsd:annotation>
      <xsd:documentation>Container for data relating to the attendance details requested for a treatment.  Including treatment care point,
											target date to come in and target breach date.
			</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" name="TreatmentCarePoint" type="sci:PatientCarePoint">
        <xsd:annotation>
          <xsd:documentation>This will represent carepoint that will provde the treatment.
							Note: For a carepoint Site = Hospital/Clinic, Location = Ward/Room</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="ToComeInTargetDate" type="xsd:date">
        <xsd:annotation>
          <xsd:documentation>Proposed Date that the patient should attend</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="ToComeInTargetTime" type="xsd:time">
        <xsd:annotation>
          <xsd:documentation>Proposed treatment time that the patient should attend</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="TreatmentBreachDate" type="xsd:date">
        <xsd:annotation>
          <xsd:documentation>Breach date</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="TreatmentRequestStatusType">
    <xsd:annotation>
      <xsd:documentation>Container for data relating to the overall status of the request to treat.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" name="RequestState" type="sci:OpenClosedStatus">
        <xsd:annotation>
          <xsd:documentation>Holds the state of the treatment request e.g Open, Closed.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="RequestStatus" type="sci:RequestStatus">
        <xsd:annotation>
          <xsd:documentation>Holds the overall status of the treatment request e.g. Outstanding, Completed, Cancelled.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="RequestStatusComment" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>Holds any additional information relating to the status of the treatment request.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:any minOccurs="0" maxOccurs="unbounded" namespace="##other" processContents="lax" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="TreatmentRequestOutcomeType">
    <xsd:annotation>
      <xsd:documentation>Container for data relating to the overall outcome relating to the request to treat.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" name="DateClosed" type="xsd:date" />
      <xsd:element minOccurs="0" name="AttendanceStatus" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>Holds the attendance status of an appointment \ visit</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="ActualAttendanceDetails">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element minOccurs="0" name="ArrivalDate" type="xsd:date">
              <xsd:annotation>
                <xsd:documentation>Arrival  Date</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="ArrivalTime" type="xsd:time">
              <xsd:annotation>
                <xsd:documentation>Arrival time</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="DepartureDate" type="xsd:date">
              <xsd:annotation>
                <xsd:documentation>Departure  Date</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="DepartureTime" type="xsd:time">
              <xsd:annotation>
                <xsd:documentation>Departure time</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="TreatmentDate" type="xsd:date">
              <xsd:annotation>
                <xsd:documentation>Actual treatment Date</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="TreatmentTime" type="xsd:time">
              <xsd:annotation>
                <xsd:documentation>Actual treatment time</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:any minOccurs="0" maxOccurs="unbounded" namespace="##other" processContents="lax" />
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element minOccurs="0" maxOccurs="unbounded" name="TreatmentReceived" type="gen:CLINICAL_CIRCUMSTANCE_TYPE" />
      <xsd:any minOccurs="0" maxOccurs="unbounded" namespace="##other" processContents="lax" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="TreatmentRequestDeliveryScheduleType">
    <xsd:annotation>
      <xsd:documentation>This container is for data relating to the delivery of the treatment. Primarily RTT details</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" name="ReferralToTreatClock">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="StartDate" type="xsd:date" />
            <xsd:element name="TargetDate" type="xsd:date" />
            <xsd:element name="Status" type="sci:GENERAL_CODE_TYPE" />
            <xsd:any minOccurs="0" maxOccurs="unbounded" namespace="##other" processContents="lax" />
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:any minOccurs="0" maxOccurs="unbounded" namespace="##other" processContents="lax" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="ADTinterface">
    <xsd:annotation>
      <xsd:documentation>Detailed ADT information</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="ADTInformation" type="sci:ADTinfoDetailed" />
      <xsd:element minOccurs="0" name="PatientInformation" type="sci:PatientInformation" />
      <xsd:element name="OriginatingSystem" type="xsd:string" />
      <xsd:element minOccurs="0" name="TransactionType" type="sci:RecordModifcationType" />
      <xsd:any minOccurs="0" maxOccurs="unbounded" namespace="##other" processContents="lax" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="ADTinfoDetailed">
    <xsd:annotation>
      <xsd:documentation>Detailed ADT information Detailed</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="ADTid" type="sci:ADTtransactionID" />
      <xsd:element minOccurs="0" name="ADTmessageContent">
        <xsd:annotation>
          <xsd:documentation>Element is not present for transactions of Type "Deletion"</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:choice>
            <xsd:element name="AdmissionContent" type="sci:ADTadmissionType" />
            <xsd:element name="TransferContent" type="sci:ADTtransferType" />
            <xsd:element name="DischargeContent" type="sci:ADTdischargeType" />
          </xsd:choice>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="TransactionDateTime" nillable="true" type="xsd:dateTime" />
      <xsd:any minOccurs="0" maxOccurs="unbounded" namespace="##other" processContents="lax" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="PatientCarePoint">
    <xsd:annotation>
      <xsd:documentation>Patient care point definition</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" name="Site" type="gen:ORGANISATION_TYPE">
        <xsd:annotation>
          <xsd:documentation>This will represent the site of patient care  Hospital/Clinic</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Location" type="gen:ORGANISATION_TYPE">
        <xsd:annotation>
          <xsd:documentation>This will represent the site of patient care  Ward/Room</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="ResponsibleHCP" type="gen:HCP_DETAIL_TYPE">
        <xsd:annotation>
          <xsd:documentation>Healthcare professional responsible for the patient care</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Clinic" type="gen:CLINIC_TYPE">
        <xsd:annotation>
          <xsd:documentation>The name of a schedule of appointments normally arranged to be held on a regular basis at a particular location.  Coded value or string.</xsd:documentation>
          <xsd:appinfo>SMR Data Manual 3-5</xsd:appinfo>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Specialty" type="gen:SPECIALTY_TYPE">
        <xsd:annotation>
          <xsd:documentation>Simple Type : A division of medicine or dentistry covering a specific area of clinical activity and identified within one of the Royal Colleges or Faculties . Ref: ISD Definitions and Codes for the Health Service in Scotland : 5-5 </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Facility" type="sci:GENERAL_CODE_TYPE">
        <xsd:annotation>
          <xsd:documentation>Facility</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:any minOccurs="0" maxOccurs="unbounded" namespace="##other" processContents="lax" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="ADTpatientCarePoint">
    <xsd:annotation>
      <xsd:documentation>ADT patient care point definition</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" name="Site" type="gen:ID_TYPE">
        <xsd:annotation>
          <xsd:documentation>e.g. Hospital</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Location" type="sci:MedicalFacility">
        <xsd:annotation>
          <xsd:documentation>e.g. Ward</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Consultant" type="gen:ID_TYPE" />
      <xsd:element minOccurs="0" name="Specialty" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>COPPISH code</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Facility" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>COPPISH code</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:any minOccurs="0" maxOccurs="unbounded" namespace="##other" processContents="lax" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="ComponentID">
    <xsd:annotation>
      <xsd:documentation>PAS Component ID</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" name="SpellOfCareID">
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:minLength value="1" />
            <xsd:maxLength value="35" />
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element minOccurs="0" name="EpisodeID">
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:minLength value="1" />
            <xsd:maxLength value="35" />
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element minOccurs="0" name="EventID">
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:minLength value="1" />
            <xsd:maxLength value="35" />
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:any minOccurs="0" maxOccurs="unbounded" namespace="##other" processContents="lax" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="ADTtransactionID">
    <xsd:annotation>
      <xsd:documentation>ADT messages from HOMER have simpleID
ADT messages from COMPAS have compoundID</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="RecordIdentifier" type="sci:ADTrecordIdentifier" />
      <xsd:element minOccurs="0" name="ADTComponentID" type="sci:ComponentID" />
      <xsd:element minOccurs="0" name="IncidenceID" type="xsd:string" />
      <xsd:element minOccurs="0" name="UniqueCarePathwayNumber" type="gen:UNIQUE_CARE_PATHWAY_NUMBER_TYPE">
        <xsd:annotation>
          <xsd:documentation>The Unique Care Pathway Number which should identify a pathway of care for the patient. Introduced as part of 18 weeks programme (see www.18weeks.scot.nhs.uk)</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:any minOccurs="0" maxOccurs="unbounded" namespace="##other" processContents="lax" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="ADTsystemInformation">
    <xsd:annotation>
      <xsd:documentation>Report related system information</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="ADTid" type="xsd:integer">
        <xsd:annotation>
          <xsd:documentation>Database ID of  testReport</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Provider" type="sci:ProviderItem" />
      <xsd:element name="PatientID" type="sci:StoreGenericIdentifier">
        <xsd:annotation>
          <xsd:documentation>Database ID of the patient on table PatientMaster</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:any minOccurs="0" maxOccurs="unbounded" namespace="##other" processContents="lax" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="ReferraltransactionID">
    <xsd:annotation>
      <xsd:documentation>Structure used to identify referrals</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="RecordIdentifier" type="sci:ADTrecordIdentifier" />
      <xsd:element minOccurs="0" name="IncidenceID" type="xsd:string" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="InpatientPassID">
    <xsd:annotation>
      <xsd:documentation>Structure used to identify inpatient Pass details</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="ADTrecordIdentifier" type="sci:ADTrecordIdentifier" />
      <xsd:element minOccurs="0" name="IncidenceID" type="xsd:string" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="InpatientPasslInfo">
    <xsd:annotation>
      <xsd:documentation>Detailed ADT information</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="InpatientPassID" type="sci:ADTtransactionID" />
      <xsd:element name="PassDate" type="xsd:dateTime" />
      <xsd:element name="ExpectedReturnDate" type="xsd:dateTime" />
      <xsd:element minOccurs="0" name="ReturnDate" type="xsd:dateTime" />
      <xsd:element minOccurs="0" name="PassReason" type="xsd:string" />
      <xsd:element minOccurs="0" name="ContactAddress" type="gen:ADDRESS_TYPE" />
      <xsd:element minOccurs="0" name="TransactionDetails">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="TransactionDateTime" nillable="true" type="xsd:dateTime" />
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:any minOccurs="0" maxOccurs="unbounded" namespace="##other" processContents="lax" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="MergePatientInterface">
    <xsd:annotation>
      <xsd:documentation>Merge Patient interface specification</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="PrimaryPatient" type="sci:PatientInformation" />
      <xsd:element name="SubordinatePatient" type="sci:PatientInformation" />
      <xsd:element name="MergeAction" type="sci:MergeAction" />
      <xsd:any minOccurs="0" maxOccurs="unbounded" namespace="##other" processContents="lax" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="UnMergePatientInterface">
    <xsd:annotation>
      <xsd:documentation>Merge Patient interface specification</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="PrimaryPatient" type="gen:ID_TYPE" />
      <xsd:element name="SubordinatePatient" type="gen:ID_TYPE" />
      <xsd:any minOccurs="0" maxOccurs="unbounded" namespace="##other" processContents="lax" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="BreakGlassInfo">
    <xsd:annotation>
      <xsd:documentation>ADT messages from HOMER have simpleID
ADT messages from COMPAS have compoundID</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="PatientID" type="sci:PatientIdentifier" />
      <xsd:element name="BreakGlassTypeCode" type="sci:BreakGlassTypeCode" />
      <xsd:element name="Reason">
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:minLength value="1" />
            <xsd:maxLength value="250" />
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:any minOccurs="0" maxOccurs="unbounded" namespace="##other" processContents="lax" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="BreakGlassItem">
    <xsd:annotation>
      <xsd:documentation>Breakglass item </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="PatientID" type="sci:PatientIdentifier" />
      <xsd:element name="BreakGlassToken" type="sci:Token" />
      <xsd:element minOccurs="0" name="BreakGlassExpires" type="xsd:dateTime" />
      <xsd:any minOccurs="0" maxOccurs="unbounded" namespace="##other" processContents="lax" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="BreakGlassItemList">
    <xsd:annotation>
      <xsd:documentation>List of breakglass items</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" maxOccurs="unbounded" name="BreakGlassItem" type="sci:BreakGlassItem" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="BreakGlassTypeItem">
    <xsd:annotation>
      <xsd:documentation>Break Glass Type</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="BreakGlassTypeID" type="sci:StoreGenericIdentifier" />
      <xsd:element name="BreakGlassTypeCode" type="sci:BreakGlassTypeCode" />
      <xsd:element name="BreakGlassTypeDescription">
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:minLength value="1" />
            <xsd:maxLength value="30" />
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:any minOccurs="0" maxOccurs="unbounded" namespace="##other" processContents="lax" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="BreakGlassTypeItemList">
    <xsd:annotation>
      <xsd:documentation>Break Glass type list</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" maxOccurs="unbounded" name="BreakGlassType" type="sci:BreakGlassTypeItem" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="BreakGlassAuditItem">
    <xsd:annotation>
      <xsd:documentation>Breakglass item </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="PatientDetails">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="PatientID" type="sci:PatientIdentifier" />
            <xsd:element minOccurs="0" name="CHI" type="sci:CHInumber" />
            <xsd:element minOccurs="0" name="DateOfBirth" type="xsd:date" />
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="BreakGlassDetails">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="Reason">
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:minLength value="1" />
                  <xsd:maxLength value="250" />
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element name="BreakGlassDateTime" type="xsd:dateTime" />
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="BreakGlassType" type="sci:BreakGlassTypeItem" />
      <xsd:element name="UserDetails" type="sci:UserInfo" />
      <xsd:any minOccurs="0" maxOccurs="unbounded" namespace="##other" processContents="lax" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="BreakGlassAuditItemList">
    <xsd:annotation>
      <xsd:documentation>Break Glass type list</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" maxOccurs="unbounded" name="BreakGlassAuditInstance" type="sci:BreakGlassAuditItem" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="FindHCPcriteria">
    <xsd:annotation>
      <xsd:documentation>Selection criteria for FindHCP</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" name="HCPcode">
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:minLength value="1" />
            <xsd:maxLength value="10" />
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element minOccurs="0" name="HCPid" type="sci:StoreGenericIdentifier" />
      <xsd:element minOccurs="0" name="Name">
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:minLength value="1" />
            <xsd:maxLength value="50" />
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element minOccurs="0" name="Position">
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:minLength value="1" />
            <xsd:maxLength value="50" />
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:any minOccurs="0" maxOccurs="unbounded" namespace="##other" processContents="lax" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="FindOrganisationCriteria">
    <xsd:annotation>
      <xsd:documentation>Selection criteria for FindOrganisationCriteria</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" name="OrganisationCode">
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:minLength value="1" />
            <xsd:maxLength value="10" />
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element minOccurs="0" name="OrganisationID" type="sci:StoreGenericIdentifier" />
      <xsd:element minOccurs="0" name="OrganisationName">
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:minLength value="1" />
            <xsd:maxLength value="50" />
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element minOccurs="0" name="OrganisationType">
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:minLength value="1" />
            <xsd:maxLength value="50" />
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:any minOccurs="0" maxOccurs="unbounded" namespace="##other" processContents="lax" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="FindBreakGlassAuditCriteria">
    <xsd:annotation>
      <xsd:documentation>Selection criteria for FindDocument</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" name="PatientID" type="sci:PatientIdentifier" />
      <xsd:element minOccurs="0" name="DateRange" type="sci:DateRangeSelection" />
      <xsd:element minOccurs="0" name="User" type="sci:FindUserCriteria" />
      <xsd:element minOccurs="0" name="TypeCode">
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:minLength value="1" />
            <xsd:maxLength value="10" />
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:any minOccurs="0" maxOccurs="unbounded" namespace="##other" processContents="lax" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="DocumentInfo">
    <xsd:annotation>
      <xsd:documentation>base definition for document information</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="SystemInformation" type="sci:DocumentSystemInformation" />
      <xsd:element name="DocumentDetails" type="sci:DocumentMetadata" />
      <xsd:element minOccurs="0" name="DocumentStylesheet" type="sci:DocumentStylesheetType" />
      <xsd:element minOccurs="0" name="PatientInformation" type="sci:PatientInformation" />
      <xsd:element name="DocumentData" type="sci:DocumentData" />
      <xsd:any minOccurs="0" maxOccurs="unbounded" namespace="##other" processContents="lax" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="DocumentMetadata">
    <xsd:annotation>
      <xsd:documentation>base definition for document metadata</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="DocumentCategory" type="sci:CLINICAL_INDEX_TYPE" />
      <xsd:element name="DocumentIdentifier" type="xsd:string" />
      <xsd:element minOccurs="0" name="UniqueCarePathwayNumber" type="gen:UNIQUE_CARE_PATHWAY_NUMBER_TYPE">
        <xsd:annotation>
          <xsd:documentation>The Unique Care Pathway Number which should identify a pathway of care for the patient. Introduced as part of 18 weeks programme (see www.18weeks.scot.nhs.uk)</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="DocumentTitle" type="xsd:string" />
      <xsd:element name="DocumentRevision" type="xsd:string" />
      <xsd:element minOccurs="0" name="DocumentSpecialty" type="gen:SPECIALTY_TYPE" />
      <xsd:element minOccurs="0" name="DocumentOrganisationName" type="xsd:string" />
      <xsd:element minOccurs="0" name="DocumentOrganisationType" type="xsd:string" />
      <xsd:element minOccurs="0" name="Stylesheet" type="xsd:string" />
      <xsd:element minOccurs="0" name="AdditionalText" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>Text normally embedded in a CDATA block</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="DocumentAttestationDateTime" type="xsd:dateTime" />
      <xsd:element minOccurs="0" name="OriginatingHcp_HealthcareProfessionalID" type="xsd:integer" />
      <xsd:element minOccurs="0" name="AttestingHcp_HealthcareProfessionalID" type="xsd:integer" />
      <xsd:element minOccurs="0" name="ReceivingHcp_HealthcareProfessionalID" type="xsd:integer" />
      <xsd:element name="CopiedHcp_HealthcareProfessionalIDs">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element minOccurs="0" maxOccurs="unbounded" name="CopiedHcp_HealthcareProfessionalID" type="xsd:integer" />
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="ConsentToShare" type="gen:DOCUMENT_PATIENT_CONSENT_TYPE">
        <xsd:annotation>
          <xsd:documentation>Y="Yes"
N= "No"
Permissions restrict "N" documents</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Sensitivity" type="gen:DOCUMENT_SENSITIVITY_TYPE">
        <xsd:annotation>
          <xsd:documentation>S="Sensitive"
HS="Highly Sensitive"
Permissions restrict HS documents</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="PackageIdentifier" type="xsd:string" />
      <xsd:element minOccurs="0" name="UserNotes" type="sci:DocumentNoteList">
        <xsd:annotation>
          <xsd:documentation>Text normally embedded in a CDATA block</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Stylesheets" type="sci:DocumentStylesheetList" />
      <xsd:element minOccurs="0" name="Keywords">
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:minLength value="1" />
            <xsd:maxLength value="255" />
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="UserNotesAvailable" type="xsd:boolean" />
      <xsd:element name="AdditionalStylesheetsAvailable" type="xsd:boolean" />
      <xsd:element name="IsRetired" type="xsd:boolean" />
      <xsd:any minOccurs="0" maxOccurs="unbounded" namespace="##other" processContents="lax" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="DocumentSearchSummary">
    <xsd:annotation>
      <xsd:documentation>This definition includes summary details for document finds</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" name="TotalDocuments" type="xsd:integer">
        <xsd:annotation>
          <xsd:documentation>The total number of documents selected according to the selection criteria</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="HiddenDocuments" type="xsd:integer">
        <xsd:annotation>
          <xsd:documentation>the number of documents hidden, i.e. which the User is not permitted to see because of restricions associated with permission settings</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="DocumentSystemInformation">
    <xsd:annotation>
      <xsd:documentation>database keys and related system information</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="PatientID" type="sci:PatientIdentifier">
        <xsd:annotation>
          <xsd:documentation>Database ID of the patient on table PatientMaster</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="PatientDocumentSearchID" type="sci:StoreGenericIdentifier">
        <xsd:annotation>
          <xsd:documentation>Database ID of row on table t_PatientDocumentSearch, which holds document metadata and link information</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Provider" type="sci:ProviderItem" />
      <xsd:element minOccurs="0" name="DocumentSourceID" nillable="true" type="sci:StoreGenericIdentifier">
        <xsd:annotation>
          <xsd:documentation>Database ID of row on table t_DocumentSource. May be null if table has been tidied to remove no-current data</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="PatientDocumentID" type="sci:StoreGenericIdentifier">
        <xsd:annotation>
          <xsd:documentation>Database ID of the document on table t_PatientDocument. Note that this may be on a remote database, or -1 if document stored on a file server</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="DocumentToDatabaseID" type="sci:StoreGenericIdentifier">
        <xsd:annotation>
          <xsd:documentation>Database ID of the Document To Database Service</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="DocumentStatusID" type="sci:StoreGenericIdentifier">
        <xsd:annotation>
          <xsd:documentation>Status of Document In Store</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="DocumentIsLinked" type="xsd:boolean">
        <xsd:annotation>
          <xsd:documentation>Indicator whether document is a linked document</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="DocumentHasHistory" type="xsd:boolean" />
      <xsd:any minOccurs="0" maxOccurs="unbounded" namespace="##other" processContents="lax" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="DocumentUpload">
    <xsd:annotation>
      <xsd:documentation>Contains information identifying a patient and a separate binary document to be uploaded</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="MessageData" type="sci:DocumentUploadMessageType">
        <xsd:annotation>
          <xsd:documentation>A container for data which comprises the message contained in the Clinical Letter. Ref: ENV 13606 - 4:2000 3.50.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="DocumentData" type="gen:DOCUMENT_DATA_TYPE">
        <xsd:annotation>
          <xsd:documentation>Container for data describing the characteristics of the document itself. Ref:ENV 13606 - 4:2000  7.5.2</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:any minOccurs="0" maxOccurs="unbounded" namespace="##other" processContents="lax" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="DocumentUploadMessageType">
    <xsd:annotation>
      <xsd:documentation>A structure for data relating to the message contained in the Clinical Letter, as opposed to the data about the document. Ref: ENV 13606 - 4:2000 3.50.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="PatientDemographics" type="sci:PatientInformation">
        <xsd:annotation>
          <xsd:documentation>Container for patient identifying details of the patient who is the subject of the document.  Ref: ENV 13606 - 4:2000 7.4.6  </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="ClinicalData">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element minOccurs="0" name="DocumentSubCategory">
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:minLength value="1" />
                  <xsd:maxLength value="255" />
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element minOccurs="0" name="PackageIdentifier" type="xsd:string" />
            <xsd:element name="DocumentSpecialty" type="gen:SPECIALTY_TYPE" />
            <xsd:element name="DocumentTitle">
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:minLength value="1" />
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element minOccurs="0" name="DocumentOrganisationName">
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:minLength value="1" />
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element minOccurs="0" name="DocumentOrganisationType">
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:minLength value="1" />
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element minOccurs="0" name="AdditionalText">
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:minLength value="1" />
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element minOccurs="0" name="Stylesheet">
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:minLength value="1" />
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element minOccurs="0" name="AdditionalStylesheets" type="sci:DocumentStylesheetList" />
            <xsd:element minOccurs="0" name="Notes" type="sci:DocumentNoteList" />
            <xsd:element minOccurs="0" name="KeywordText" type="xsd:string" />
            <xsd:any minOccurs="0" maxOccurs="unbounded" namespace="##other" processContents="lax" />
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:any minOccurs="0" maxOccurs="unbounded" namespace="##other" processContents="lax" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="FindDocumentItem">
    <xsd:annotation>
      <xsd:documentation>This definition specified the information return during document find</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="SystemInformation" type="sci:DocumentSystemInformation" />
      <xsd:element name="DocumentDetails" type="sci:DocumentMetadata" />
      <xsd:element minOccurs="0" name="PatientDetails" type="sci:FindPatientItem" />
      <xsd:any minOccurs="0" maxOccurs="unbounded" namespace="##other" processContents="lax" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="TreatmentLogSystemInformation">
    <xsd:annotation>
      <xsd:documentation>database keys and related system information</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="PatientID" type="sci:PatientIdentifier">
        <xsd:annotation>
